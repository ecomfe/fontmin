  "types": "index.d.ts",
  "keywords": [
    "font",
    "webfont",
    "icon",
    "iconfont",
    "font-face",
    "compress",
    "minify",
    "font-cli",
    "otf",
    "ttf",
    "woff",
    "woff2",
    "eot",
    "svg",
    "ttf2eot",
    "ttf2woff",
    "ttf2woff2",
    "ttf2svg",
    "svg2ttf",
    "css",
    "base64"
  ],
  "author": "junmer",
  "license": "MIT",
  "repository": "ecomfe/fontmin",
  "engines": {
    "node": ">=12"
  },
  "bin": {
