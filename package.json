    "meow": "^9.0.0",
    "pako": "^2.0.3",
    "replace-ext": "^2.0.0",
    "stream-combiner": "^0.2.1",
    "through2": "^4.0.2",
    "ttf2woff2": "^4.0.1",
    "vinyl-fs": "^3.0.3"
